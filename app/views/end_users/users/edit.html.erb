<h1>会員情報編集</h1>
<%= form_with model: @user, url:end_users_users_path, local:true do |f| %>
<table>
  <tr>
    <th>名前</th>
    <td><%= f.text_field :last_name %><%= f.text_field :fitst_name %></td>
  </tr>
  <tr>
    <th>カナ</th>
    <td><%= f.text_field :last_name_kana %><%= f.text_field :first_name_kana %></td>
  </tr>
  <tr>
    <th>メールアドレス</th>
    <td><%= f.text_field :email %></td>
  </tr>
  <tr>
    <th>パスワード</th>
    <td><%= f.text_field :password %></td>
  </tr>
  <%= f.submit '変更を保存' %>
</table>
<% end %>