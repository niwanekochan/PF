<h1>ジャンル追加/一覧</h1>
<%= form_with model: @genre, url:admins_genres_path, local:true do |f| %>
<table>
  <tr>
    <th>ジャンル名</th>
    <td><%= f.text_field :name %></td>
    <td><%= f.submit %></td>
  </tr>
</table>
<% end %>

<table>
  <thead>
    <tr>
      <th>ジャンル名</th>
      <th></th>
    </tr>
  </thead>
  <% @genres.each do |genre| %>
  <tbody>
    <tr>
      <td><%= genre.name %></td>
      <td><%= link_to '編集', edit_admin_genre_path(genre.id) %></td>
    </tr>
  </tbody>
  <% end %>
</table>